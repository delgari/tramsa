<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Reportes</title>
    <%- include ('../partials/head') %>
</head>
<body>
    <div class="wrapper">
        <%- include ('../partials/navegacion') %>
         <!-- Contenido general -->
        <div id="content">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">

                    <button type="button" id="sidebarCollapse" class="navbar-btn">
                        <span></span>
                        <span></span>
                        <span></span>
                    </button>
                    <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse"
                        data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <i class="fas fa-align-justify"></i>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="nav navbar-nav ml-auto">
                            <li class="nav-item active">
                                <img src="../../Imagenes/Logo2.png" width="130" height="50" />
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- Formulario -->
            <div class="container">
                <section>
                    <ul id="da-thumbs" class="da-thumbs">
                        <li>
                                <img src="../../Imagenes/proTramBlack.jpg">
                                <div style="display: block; left: 0px; top: 100%; overflow: hidden; transition: all 300ms ease 0s;"><span>Menu by Simon Jensen</span></div>
                         </li>
                        <li>
                                <img src="../../Imagenes/inventarioBlack.jpg">
                                <div style="display: block; left: -100%; top: 0px; overflow: hidden; transition: all 300ms ease 0s;"><span>TN Aquarium by Charlie Gann</span></div>
                          </li>
                        <li>
                                <img src="../../Imagenes/ventasBlack.jpg">
                                <div style="display: block; left: 0px; top: -100%; overflow: hidden; transition: all 300ms ease 0s;"><span>Mr. Crabs by John Generalov</span></div>
                         </li>
                        <li>
                                <img src="../../Imagenes/listadoBlack.jpg">
                                <div style="display: block; left: -100%; top: 0px; overflow: hidden; transition: all 300ms ease 0s;"><span>Gallery of Mo 2.Mo logo by Adam Campion</span></div>
                        </li>
                    </ul>
                </section>
            </div>

        </div>

    </div>

</body>
</html>
<script type="text/javascript">
    $(document).ready(function () {
        $("#buscar").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#bodegas tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });

    $(function() {
    
    $(' #da-thumbs > li ').each( function() { $(this).hoverdir(); } );

    });
</script>
<script src="https://tympanus.net/TipsTricks/DirectionAwareHoverEffect/js/jquery.hoverdir.js"></script>
<script src="https://tympanus.net/TipsTricks/DirectionAwareHoverEffect/js/modernizr.custom.97074.js"></script>

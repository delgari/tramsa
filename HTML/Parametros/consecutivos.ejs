<!DOCTYPE html>
<html lang="en">

<head>
    <title>Consecutivos</title>
    <%- include ('../partials/head') %>
</head>

<body>
    <div class="wrapper">
        <%- include ('../partials/navegacion') %>
        <!-- Contenido general -->
        <div id="content">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">

                    <button type="button" id="sidebarCollapse" class="navbar-btn">
                        <span></span>
                        <span></span>
                        <span></span>
                    </button>
                    <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse"
                        data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <i class="fas fa-align-justify"></i>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="nav navbar-nav ml-auto">
                            <li class="nav-item active">
                                <img src="../../Imagenes/Logo2.png" width="130" height="50" />
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- Información -->
            <div class="container">
                <div class="row">
                    <div class="col-sm-10 col-md-7 col-lg-10 mx-auto">
                        <div class="card card-signin my-3">
                            <div class="card-body">
                                <h4 class="card-title text-center">Consecutivos</h4>
                                </br>
                                <form>
                                    <div class="form-row">
                                        <div class="col">
                                            <label for="prefijo">Prefijo</label>
                                            <select id="prefijo" class="form-control">
                                              <option selected>Buscar...</option>
                                              <option>TMP</option>
                                              <option>BO</option>
                                              <option>MP</option>
                                              <option>PR</option>
                                              <option>US</option>
                                              <option>CL</option>
                                              <option>FC</option>
                                              <option>PP</option>
                                              <option>CA</option>
                                              <option>PRO</option>
                                              <option>FAC</option>
                                            </select>
                                          </div>
                                          <div class="col">
                                            <label for="descripcion">Descripción</label>
                                            <select id="descripcion" class="form-control">
                                              <option selected>Buscar...</option>
                                              <option>Tipos Materia Prima</option>
                                              <option>Bodegas</option>
                                              <option>Materia Prima</option>
                                              <option>Productos</option>
                                              <option>Usuarios</option>
                                              <option>Clientes</option>
                                              <option>Facturas</option>
                                              <option>Pedidos de Productos</option>
                                              <option>Camiones</option>
                                              <option>Proveedores</option>
                                              <option>Facturas</option>
                                            </select>
                                          </div>
                                          <div class="col">
                                            <fieldset disabled>
                                            <label for="valor">Valor</label>
                                            <input type="number" class="form-control" id="valor">
                                            </fieldset>
                                          </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>